<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="400.0" prefWidth="640.0" stylesheets="@../../style.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pi1.GUI.commentaireController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
         <children>
            <Label layoutX="230.0" layoutY="40.0" text="FeedBack :" textFill="#354d23f0">
               <font>
                  <Font name="Bodoni MT Black" size="32.0" />
               </font>
            </Label>
            <TextField fx:id="comied" layoutX="24.0" layoutY="146.0" prefHeight="85.0" prefWidth="410.0" promptText="Ajoutez votre commentaire :" style="-fx-background-color: #ffffffff;" />
            <Button fx:id="ajty" layoutX="476.0" layoutY="295.0" mnemonicParsing="false" onAction="#ajouter" prefHeight="25.0" prefWidth="74.0" style="-fx-background-color: #e9edceff;" text="Ajouter " />
            <Button fx:id="modify" layoutX="557.0" layoutY="295.0" mnemonicParsing="false" onAction="#modifier" prefHeight="25.0" prefWidth="74.0" style="-fx-background-color: #e9edceff;" stylesheets="@../../style.css" text="Modifier " />
            <Button fx:id="affichery" layoutX="557.0" layoutY="352.0" mnemonicParsing="false" onAction="#affciher" prefHeight="25.0" prefWidth="74.0" style="-fx-background-color: #e9edceff;" text="Afficher " />
            <Button fx:id="supprimery" layoutX="476.0" layoutY="352.0" mnemonicParsing="false" onAction="#supprimer" style="-fx-background-color: #e9edceff;" text="supprimer " />
            <TableView fx:id="tvtablec" layoutX="24.0" layoutY="252.0" prefHeight="136.0" prefWidth="444.0" style="-fx-background-color: #e9edceff;">
              <columns>
                <TableColumn fx:id="date" prefWidth="108.0" text="date " />
                <TableColumn fx:id="text" prefWidth="311.0" text="text" />
              </columns>
            </TableView>
            <DatePicker fx:id="datey" layoutX="464.0" layoutY="151.0" prefHeight="75.0" prefWidth="150.0" style="-fx-background-color: #e7edc9ff;" />
            <Label layoutX="45.0" layoutY="111.0" text="Commentaire :">
               <font>
                  <Font name="System Bold Italic" size="18.0" />
               </font>
            </Label>
            <Label layoutX="513.0" layoutY="109.0" text="Date :">
               <font>
                  <Font name="System Bold Italic" size="17.0" />
               </font>
            </Label>
         </children>
    </AnchorPane>
  </children>
</VBox>
